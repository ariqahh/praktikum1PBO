
package scannerpola1;

import java.util.Scanner;

public class Scannerpola1 {

    
    public static void main(String[] args) {
        Scanner input = new Scanner (System.in);
        int angka = input.nextInt();
        for (int i = 0; i < angka; i++) {
            System.out.print("* ");
        }
        System.out.println();
        
    }
    
}
