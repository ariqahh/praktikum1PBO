
package pola2;

import java.util.Scanner;

public class Pola2 {

   
    public static void main(String[] args) {
        Scanner input = new Scanner (System.in);
        int angka = input.nextInt();
        for (int i = 0; i < angka; i++) {
            System.out.println("* ");
            
        }
        System.out.println();
        
        
    }
    
}

