
package pola24;

import java.util.Scanner;

public class Pola24 {

    public static void main(String[] args) {
        Scanner input = new Scanner (System.in);
        int angka = input.nextInt(); //baris
        int angka1 = input.nextInt();//kolom
        for (int i = 0; i < angka; i++) {
            for (int j = 0; j < angka1; j++) {
                System.out.print("+ ");
            }
            System.out.println();
        }
    }
    
}
