
package pola5;

import java.util.Scanner;

public class Pola5 {

    public static void main(String[] args) {
        Scanner input = new Scanner (System.in);
        int angka = input.nextInt();
        for (int i = angka; i > 0; i--) { //for down to
            for (int j = 0; j < i; j++) {
                System.out.print("* ");
                
            }
            System.out.println();
        }
    }
    
}
